-- learn more on https://docs.dataform.co/guides/incremental-tables/

config { 
    type: "incremental",
    tags: ["advanced"]
}

SELECT current_date as snapshot_date,
       country

FROM ${ref("dataset_2_with_ref")}

GROUP BY 1, 2

incremental_where {
    snapshot_date > (select max(snapshot_date) from ${self()})
    
}