config {
  type: "assertion",
  tag: "advanced"
}

-- learn more on https://docs.dataform.co/guides/assertions/

with row_count as (
  SELECT
    id,
    email,
    ROW_NUMBER() OVER (PARTITION BY id) as row_number
  FROM
    ${ref("customers")}
)


SELECT
  id,
  email
FROM
  row_count
WHERE
  row_number > 1
